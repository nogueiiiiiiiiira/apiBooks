generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

//npx prisma migrate dev
//prisma db push

model Book {
  id Int @id @default(autoincrement())
  nome String
  descricao String
  autor String
  categoria String
  estoque String
  criadoEm String
}

model Librarian {
  id Int @id @default(autoincrement())
  nome String
  cpf String
  email String
  telefone String
  dataNasc String
  senha String
  criadoEm String
}

model Loan {
  id Int @id @default(autoincrement())
  cpf String
  idLivro String
  nomeLivro String
  dataEmp String
  dataDev String
}

model Reader {
  id Int @id @default(autoincrement())
  nome String
  cpf String
  email String
  telefone String
  dataNasc String
  criadoEm String
}

model Return {
  id Int @id @default(autoincrement())
  cpf String
  idLivro String
  prevDev String
  dataAtual String
  multaAtribuida String
}

model Fine {
  id Int @id @default(autoincrement())
  cpf String
  idLivro String
  diasAtra String
  total String
  statusPag String
  criadoEm String
}

model Historic {
  id Int @id @default(autoincrement())
  acao String
  criadoEm String
}